# Oasis Sapphire Configuration
# ---------------------------
# Copy this file to .env and fill in your values

# Your Ethereum private key for signing transactions on Sapphire
# This should be a private key for a wallet with ROSE tokens
OASIS_PRIVATE_KEY=your_private_key_here

# Network configuration (use one of: testnet, mainnet, localnet)
OASIS_NETWORK=testnet

# Terra API Configuration
# ----------------------
# API key for Terra API integration
TERRA_API_KEY=your_terra_api_key_here

# Service Configuration
# -------------------
# Port for the service API
SERVICE_PORT=3000

# Default evaluator type (rule_based or llm)
DEFAULT_EVALUATOR=rule_based

# Contract addresses (fill after deployment)
DEPOSIT_CONTRACT_ADDRESS=
PROMISE_KEEPER_ADDRESS=

# LLM Configuration (if using LLM evaluator)
LLM_API_KEY=
LLM_MODEL=gpt-4 